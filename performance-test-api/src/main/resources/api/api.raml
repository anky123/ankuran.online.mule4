#%RAML 1.0
baseUri: https://mocksvc.mulesoft.com/mocks/c5f55550-8d61-424a-9301-25d14b0fd50f
title: performance-test-api
description: This REST API is to test performance
version: 1.0

schemas:
  createEmployeeSchema: !include schemas/createEmployee-schema.json
           
  
/createEmployee:
    post:
      description: Post Employee details
      body:
        application/json:
          schema: createEmployeeSchema
      responses:
        201:
          body:
            application/json:
              example:
                {
                   "createdId": "123e4567-e89b-12d3-a456-42661417",
                   "creationTime": "2019-08-26T13:32:10.64-07:00",
                   "message": "Employee details stored successfully"
                }
        400:
          body:
            application/json:
              example:
                {
                  "code": 1000,
                   "message": "Employee details can not store"
                }